public static void main(String[] args) {
		String str="jenifer";
		int start=0;
		int j=0;
		int f=1;
		int end=str.length()-1;
		int mid=(start+end)/2;
		for(int i=0;i<=mid;i++)
		{
			int k=mid;
			while(i<k+1)
			{
				System.out.print(str.charAt(j));
				k--;
			}
			j++;
		}
		for(int i=mid+1;i<str.length();i++)
		{
			int k=mid;
			while(k<=i)
			{
				System.out.print(str.charAt(mid+f));
				k++;
			}
			f++;
		}
