 if((nums[0]==0)&&(nums.length==1))
       {
           return true;
       }
        for(int i=0;i<nums.length;i++)
        {
            if(nums[i]==0)
            {
                return false;
            }
             if(nums[i]<=nums.length-1)
            {
                i=i+nums[i];
            }
            if(i==nums.length-1)
            {
                return true;
            }
        }
       return false; 
